<h1 class="user_heading mt-4">User Data for <%= user.fullName%></h1>
<table class="table">
    <tr>
        <th>Name</th>
        <td><%= user.fullName %></td>
    </tr>
    <tr>
        <th>Email</th>
        <td><%= user.email %></td>
    </tr>
    <tr>
        <th>Zip Code</th>
        <td><%= user.zipCode %></td>
    </tr>
    <% if(user.subscribedAccount) { %>
    <tr>
        <th>This user has a &nbsp;</th>
        <td>
            <a href="<%= `../subscribers/${user.subscribedAccount}` %>">subscribed account</a>
        </td>
    </tr>
    <% } %>
</table>